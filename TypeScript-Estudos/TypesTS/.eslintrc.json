{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-restricted-syntax": 0,
      "guard-for-in": 0,
      "prefer-const": "off"
    }
}
